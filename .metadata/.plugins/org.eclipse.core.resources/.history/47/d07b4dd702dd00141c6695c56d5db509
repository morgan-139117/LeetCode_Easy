package easy;

public class ReadOff {

	 
    public String readoff(int sec){
        for()
    }
    
    public String countAndSay(int n) {
            	
    	if( n == 0) return new String("0");
        if( n == 1) return new String("1)");
        char[] sec = {1};
        for( int i = 0; i < n ; i ++){
            sec = readoff(sec);
            
        }
        return new String(sec);
    }
    
    public static void main(String[] args){
    	
    }
}
