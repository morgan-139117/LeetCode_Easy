package easy;

public class ReadOff {

	 
    public String readoff(int sec){
        for()
    }
    
    public String countAndSay(int n) {
        if(int n == 0) return 0;
        if(int n == 1) return 1;
        char[] sec = {1};
        for( int i = 0; i < n ; i ++){
            sec = readoff(sec);
            
        }
        return new String(sec);
    }
    
    public static void main(String[] args){
    	
    }
}
